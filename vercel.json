{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "functions": {
    "app/**": {
      "maxDuration": 30
    }
  },
  "crons": [],
  "redirects": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1",
      "permanent": false
    }
  ],
  "env": {
    "NEXT_PUBLIC_APP_NAME": "Limpia Service Engine"
  }
}